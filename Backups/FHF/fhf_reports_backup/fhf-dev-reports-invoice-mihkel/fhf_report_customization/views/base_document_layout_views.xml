<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_base_document_layout_fhf" model="ir.ui.view">
        <field name="name">Document Layout FHF</field>
        <field name="model">base.document.layout</field>
        <field name="inherit_id" ref="web.view_base_document_layout"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='report_footer']" position="after">
                <field name="footer_address"/>
            </xpath>
            <xpath expr="//field[@name='report_footer']" position="replace" />

        </field>
    </record>

</odoo>